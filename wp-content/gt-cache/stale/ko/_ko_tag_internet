                    nodes[i].parentNode.href = url + '#disqus_thread';
                }
            }
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + disqus_shortname + '.disqus.com/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    //]]>
    </script><nav id=nav-below><h3 class=assistive-text> <span class="notranslate"  > 게시물 네비게이션</span> </h3><div class=nav-previous> <span class="notranslate"  > <a href="http://tatematsu.jp/ko/tag/internet/page/2/" ><span class=meta-nav>←</span> 이전 게시물</a></span> </div><div class=nav-next></div></nav><!-- #nav-above --></div><!-- #content --></section><!-- #primary --></div><!-- end of main //--><div id=sidebar><div id=secondary class=widget-area role=complementary><aside id=text-2 class="widget widget_text"><h3 class=widget-title> <span class="notranslate"  > 최신 정보를 메일로 알려</span> </h3><div class=textwidget><!-- Begin MailChimp Signup Form --><link href=http://cdn-images.mailchimp.com/embedcode/slim-081711.css rel=stylesheet type=text/css><style type=text/css>
