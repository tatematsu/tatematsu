                    nodes[i].parentNode.setAttribute('data-disqus-identifier', nodes[i].getAttribute('rel'));
                    url = nodes[i].parentNode.href.split('#', 1);
                    if (url.length == 1) { url = url[0]; }
                    else { url = url[1]; }
                    nodes[i].parentNode.href = url + '#disqus_thread';
                }
            }
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + 'disqus.com/forums/' + disqus_shortname + '/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    //]]>
    </script><nav id=nav-below><h3 class=assistive-text> <span class="notranslate"  > Начать навигацию</span> </h3><div class=nav-previous> <span class="notranslate"  > <a href="http://tatematsu.jp/ru/tag/webmarketing/page/4/?pcview=true" ><span class=meta-nav>←</span> К предыдущему сообщения</a></span> </div><div class=nav-next> <span class="notranslate"  > <a href="http://tatematsu.jp/ru/tag/webmarketing/page/2/?pcview=true" >Следующие записи <span class=meta-nav>→</span></a></span> </div></nav><!-- #nav-above --></div><!-- #content --></section><!-- #primary --></div><!-- end of main //--><div id=sidebar><div id=secondary class=widget-area role=complementary><aside id=text-2 class="widget widget_text"><h3 class=widget-title> <span class="notranslate"  > В письме вы последняя информация</span> </h3><div class=textwidget><!-- Begin MailChimp Signup Form --><link href=http://cdn-images.mailchimp.com/embedcode/slim-081711.css rel=stylesheet type=text/css><style type=text/css>
