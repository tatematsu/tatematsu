            for (var i = 0, url; i < nodes.length; i++) {
                if (nodes[i].className.indexOf('dsq-postid') != -1) {
                    nodes[i].parentNode.setAttribute('data-disqus-identifier', nodes[i].getAttribute('rel'));
                    url = nodes[i].parentNode.href.split('#', 1);
                    if (url.length == 1) { url = url[0]; }
                    else { url = url[1]; }
                    nodes[i].parentNode.href = url + '#disqus_thread';
                }
            }
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + disqus_shortname + '.disqus.com/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    //]]>
    </script><nav id=nav-below><h3 class=assistive-text> <span class="notranslate"  > Mensaje de navegación</span> </h3><div class=nav-previous> <span class="notranslate"  > <a href="http://tatematsu.jp/es/author/naofumi/page/10/" ><span class=meta-nav>←</span> Entradas más antiguas</a></span> </div><div class=nav-next> <span class="notranslate"  > <a href="http://tatematsu.jp/es/author/naofumi/page/8/" >Entradas más nuevas <span class=meta-nav>→</span></a></span> </div></nav><!-- #nav-above --></div><!-- #content --></section><!-- #primary --></div><!-- end of main //--><div id=sidebar><div id=secondary class=widget-area role=complementary><aside id=text-2 class="widget widget_text"><h3 class=widget-title> <span class="notranslate"  > Aviso por e-mail la información más reciente</span> </h3><div class=textwidget><!-- Begin MailChimp Signup Form --><link href=http://cdn-images.mailchimp.com/embedcode/slim-081711.css rel=stylesheet type=text/css><style type=text/css>
