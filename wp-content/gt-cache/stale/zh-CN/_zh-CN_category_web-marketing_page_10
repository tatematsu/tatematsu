                    if (url.length == 1) { url = url[0]; }
                    else { url = url[1]; }
                    nodes[i].parentNode.href = url + '#disqus_thread';
                }
            }
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + 'disqus.com/forums/' + disqus_shortname + '/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    //]]>
    </script></div><!-- end of main //--><div id=sidebar><div id=secondary class=widget-area role=complementary><aside id=text-2 class="widget widget_text"><h3 class=widget-title> <span class="notranslate"  >电子邮件您的最新信息</span> </h3><div class=textwidget><!-- Begin MailChimp Signup Form --><link href=http://cdn-images.mailchimp.com/embedcode/slim-081711.css rel=stylesheet type=text/css>
